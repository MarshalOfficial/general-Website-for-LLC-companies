.EncTree
{
	overflow: auto;
	border-radius: 4px;
	background-color: #f5f5f5;
}
.treeHeader
{
    border-bottom: 1px solid #dddddd;
    font-size: 18px;
}
.Tree
{
	margin-top: 20px;
}
.Tree li
{
    padding: 14px;

    font-size: 17px;
    color: rgba(0, 0, 0, 0.87);
	
    border-radius: 4px;

    list-style-type: none;
}
.nodePlace
{
    // border: 2.5px solid;
    // border-radius: 4px;
    // color: white !important;
    background-color: rgb(215,215,215);
    cursor: pointer;
}
.NPUnselected
{
    box-shadow: inset 0 0 10px 1px #0275d8;
}
.NPSelected
{
    box-shadow: inset 0 0 10px 1px green;
}

#chevron
{
    float: left;
    margin-top: 15px;
    margin-left: 5px;
    font-size: 12px;
}
/****************** ChooseNodeTree ******************/
.choosePart
{
    display: inline-block;
    min-width: 25%;
    padding: 3px;
    border-radius: 4px 1px 1px 4px;
    background-color: #e1e1e1;
    cursor: pointer;
}
.choosePart .onwer
{
    float: left;
    margin-top: 10px;
    margin-left: 10px;
    font-size: 15px;
    color: #0275d8;
}

.checkButton
{
    font-size: 18px;
    border-radius: 3px 3px 3px 3px;
    background-color: gray;
    cursor: pointer;
}
.checkButton.selected
{
    background-color: green;
}
.checkButton .fa-square 
{
    color: gray;
}
.checkButton .fa-square.selected
{
    color: green;
}
.checkButton .fa-circle
{
    color: #f9f9f9;
    font-size: 16px;
}
.checkButton .fa-check
{
    color: #f9f9f9;
    font-size: 14px;
}