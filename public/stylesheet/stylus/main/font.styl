//iransans css
@font-face
{
	font-family: IRANSans;
	font-style: normal;
	font-weight: 900;
	src: url('/stylesheet/fonts/IRANSans/eot/IRANSansWeb_Black.eot');
	src: url('/stylesheet/fonts/IRANSans/woff2/IRANSansWeb_Black.woff2') format('woff2'),
		url('/stylesheet/fonts/IRANSans/ttf/IRANSansWeb_Black.ttf') format('truetype');
}

@font-face
{
	font-family: IRANSans;
	font-style: normal;
	font-weight: bold;
	src: url('/stylesheet/fonts/IRANSans/eot/IRANSansWeb_Bold.eot');
	src: url('/stylesheet/fonts/IRANSans/woff2/IRANSansWeb_Bold.woff2') format('woff2'),
		url('/stylesheet/fonts/IRANSans/ttf/IRANSansWeb_Bold.ttf') format('truetype');
}

@font-face
{
	font-family: IRANSans;
	font-style: normal;
	font-weight: 500;
	src: url('/stylesheet/fonts/IRANSans/eot/IRANSansWeb_Medium.eot');
	src: url('/stylesheet/fonts/IRANSans/woff2/IRANSansWeb_Medium.woff2') format('woff2'),
		url('/stylesheet/fonts/IRANSans/ttf/IRANSansWeb_Medium.ttf') format('truetype');
}

@font-face
{
	font-family: IRANSans;
	font-style: normal;
	font-weight: normal;
	src: url('/stylesheet/fonts/IRANSans/eot/IRANSansWeb.eot');
	src: url('/stylesheet/fonts/IRANSans/woff2/IRANSansWeb.woff2') format('woff2'),
		url('/stylesheet/fonts/IRANSans/ttf/IRANSansWeb.ttf') format('truetype');
}

//Material css
@font-face
{
	font-family: 'Material Icons';
	font-style: normal;
	font-weight: 400;
	src: url('/framework/material-design-icons-3.0.1/iconfont/MaterialIcons-Regular.woff2');
	src: local('Material Icons'),
		local('MaterialIcons-Regular'),
		url('/framework/material-design-icons-3.0.1/iconfont/MaterialIcons-Regular.ttf') format('truetype');
}

.FontIranSans
{
	font-family: 'IRANSans' !important;
}

.FontIranSansBold
{
	font-family: 'IRANSans' !important;
	font-weight: bold;
}

.AllFontsinOrder
{
	font-family: 'IRANSans', --apple-system, BlinkMacSystemFont, Roboto !important;
}

.material-icons
{
	font-family: 'Material Icons';
	font-weight: normal;
	font-style: normal;
	font-size: 24px;
	display: inline-block;
	line-height: 1;
	text-transform: none;
	letter-spacing: normal;
	word-wrap: normal;
	white-space: nowrap;
	direction: ltr;
	-webkit-font-smoothing: antialiased;
	text-rendering: optimizeLegibility;
	-moz-osx-font-smoothing: grayscale;
	font-feature-settings: 'liga';
}